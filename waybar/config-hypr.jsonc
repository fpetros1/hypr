[
    {
        "name": "m3440x1440",
        "output": "DP-1",
        "layer": "top",
        "position": "top",
        "height": 55,
        "margin-left": 10,
        "margin-bottom": 0,
        "margin-right": 10,
        "spacing": 5,
        "modules-left": [
            //"custom/rofi",
            "hyprland/workspaces",
            "hyprland/window"
            //"hyprland/submap",
            //"idle_inhibitor",
            //"mpd"
            //"custom/media"
        ],
        "modules-center": [
            "clock#date",
            "custom/weather",
            "custom/pacman"
            //"custom/gammastep"
        ],
        "modules-right": [
            "cpu",
            "custom/gpu",
            "temperature",
            //"memory",
            //"battery",
            //"pulseaudio",
            "wireplumber",
            "tray",
            "custom/notifications"
        ], 
        "hyprland/workspaces": {
            "all-outputs": true,
            "format": "{name}",
            "format-icons": {
                "1": "‰∏Ä",
                "2": "‰∫å",
                "3": "‰∏â",
                "4": "Âõõ",
                "5": "‰∫î",
                "6": "ÂÖ≠",
                "7": "‰∏É",
                "8": "ÂÖ´",
                "9": "‰πù",
                "10": "ÂçÅ"
            },
            "on-scroll-up": "hyprctl dispatch workspace e+1 1>/dev/null",
            "on-scroll-down": "hyprctl dispatch workspace e-1 1>/dev/null",
            "sort-by-number": true,
            "active-only": false,
            "show-special": false
        },
        "hyprland/window": {
            "max-length": 100,
            "separate-outputs": true
        },
        "clock#date": {
            "format": "üïì  {:%H:%M  üìÜ  %e %b}",
            "tooltip-format": "<big>{:%Y %B}</big>\n<tt><small>{calendar}</small></tt>",
            "today-format": "<b>{}</b>"
        },
        "custom/pacman": {
            "format": "<big>Û∞èî</big>  {}",
            "interval": 600,
            "tooltip": true,
            "return-type": "json",
            "exec": "get-updates-json-arch",       // # of updates
            "on-click": "update-waybar 9",
            "signal": 9,
            "max-length": 15,
            "min-length": 5
        },
        "custom/weather": {
            "format": "{}",
            "format-alt": "{alt}: {}",
            "format-alt-click": "click-right",
            "interval": 3600,
            "exec": "curl -s 'https://wttr.in/?format=1'",
            "exec-if": "ping wttr.in -c1"
        },
        "cpu": {
            "interval": 1,
            "format": "<big>Ôãõ </big> {max_frequency}GHz <span color=\"darkgray\">| {usage}%</span>",
            "min-length": 13
        },
        "custom/gpu": {
            "exec": "print-gpu-stats",
            "format": "{}",
            "return-type": "",
            "interval": 1
        },
        "wireplumber": {
            "on-click": "pavucontrol",
            "on-click-right": "amixer sset Master toggle 1>/dev/null",
            "format": "<span foreground='#fab387'>{icon}</span>  {volume}%",
            "format-muted": "Ôö© ",
            "format-source": "ÔÑ∞",
            "format-source-muted": "ÔÑ±",
            "format-icons": {
                "headphone": "ÔÄ• ",
                "hands-free": "Ôñê ",
                "headset": "Ôñê ",
                "phone": "ÔÇï ",
                "portable": "ÔÇï ",
                "car": "ÔÜπ ",
                "default": ["ÔÄ¶ ", "ÔÄß ", "ÔÄ® "]
            }
        },
        "tray": {
            "icon-size": 18,
            "spacing": 5
        },
       "custom/notifications":{
          "tooltip":false,
          "format":"{icon}  {}",
          "format-icons":{
             "notification":"ÔÉ≥<span foreground='red'><sup>ÔëÑ</sup></span>",
             "none":"ÔÉ≥",
             "dnd-notification":"Ôá∂<span foreground='red'><sup>ÔëÑ</sup></span>",
             "dnd-none":"Ôá∂",
             "inhibited-notification":"ÔÉ≥<span foreground='red'><sup>ÔëÑ</sup></span>",
             "inhibited-none":"ÔÉ≥",
             "dnd-inhibited-notification":"Ôá∂<span foreground='red'><sup>ÔëÑ</sup></span>",
             "dnd-inhibited-none":"Ôá∂"
          },
          "return-type":"json",
          "exec-if":"which swaync-client",
          "exec":"swaync-client -swb",
          "on-click":"swaync-client -d -sw",
          "escape":true
       }
    },
